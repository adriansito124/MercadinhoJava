<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="150.0" prefWidth="275.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.desktopapp.MarketSceneController">
  <children>
    <MenuBar prefHeight="0.0" prefWidth="640.0" VBox.vgrow="NEVER" />
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="142.0" prefWidth="275.0" VBox.vgrow="ALWAYS">
         <children>
            <Button id="myProduct" fx:id="myProduct" layoutX="148.0" layoutY="71.0" mnemonicParsing="false" onMouseClicked="#meusprodutos" text="MEUS PRODUTOS" />
            <Label layoutX="60.0" layoutY="14.0" text="MERCADINHO">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Button id="newProduct" fx:id="newProduct" layoutX="14.0" layoutY="71.0" mnemonicParsing="false" onMouseClicked="#novoproduto" text="NOVO PRODUTO" />
            <Button id="goHome" fx:id="goHome" layoutX="2.0" layoutY="2.0" mnemonicParsing="false" onMouseClicked="#casa" text="Sair" />
            <Button id="delete" fx:id="delete" layoutX="77.0" layoutY="111.0" mnemonicParsing="false" onMouseClicked="#deletar" text="DELETAR PRODUTO" />
         </children></AnchorPane>
  </children>
</VBox>
